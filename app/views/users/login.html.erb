
<%= form_tag login_path do %>
  <p>
    <%= label_tag :login, "Username" %><br />
    <%= text_field_tag :user_name, params[:user_name] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p><%= submit_tag "Log in" %>
  <%= link_to "forgot password"%></p>
<% end %>
<p> New user?<%= link_to "sign up", signup_path%>